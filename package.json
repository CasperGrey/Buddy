{
  "name": "buddy-chat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "react-scripts start",
    "server": "ts-node -P server/tsconfig.json server/server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "react-scripts build",
    "build:server": "tsc -P server/tsconfig.json",
    "start:prod": "cross-env NODE_ENV=production node build/server/server.js",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --ci --coverage --watchAll=false",
    "test:update": "react-scripts test --updateSnapshot",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/test-utils.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@auth0/auth0-react": "^2.3.0",
    "@azure/arm-resources": "^5.2.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/karla": "^5.0.18",
    "@fontsource/poppins": "^5.0.8",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@octokit/rest": "^21.1.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/react": "^14.2.1",
    "@types/prismjs": "^1.26.3",
    "@types/react-syntax-highlighter": "^15.5.11",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "aria-query": "^5.1.3",
    "deep-equal": "^2.2.3",
    "es-get-iterator": "^1.1.3",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "formidable": "^2.1.2",
    "hexoid": "^1.0.0",
    "is-arguments": "^1.2.0",
    "mongodb": "^6.3.0",
    "node-fetch": "^3.3.2",
    "object-is": "^1.1.6",
    "openai": "^4.80.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "redis": "^4.6.12",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^5.1.1",
    "stop-iteration-iterator": "^1.1.0",
    "superagent": "^8.1.2",
    "supertest": "^6.3.4",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.16",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/redux-logger": "^3.0.12",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.14",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "redux-logger": "^3.0.6",
    "sodium-native": "^4.3.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tweetsodium": "^0.0.4",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "typescript": "^4.9.5"
  }
}
